<div class="container" id="sign-up-form">
  <div class="row">
    <div class="col-sm-6 col-sm-offset-1">
<h2 class="text-left"><%= @current_user.name %>'s new review!</h2>


<%= bootstrap_form_for @review do |f| %>

<% if @review.errors.any? %>
<ul>
  <% @review.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>
<% end %>

<h3><%= @club.name %></h3>
<%= f.hidden_field :club_id, value: params[:club_id]%>


<%= f.date_field :date %>



<%= f.hidden_field :user_id, value: session[:user_id]%>




<%= f.text_area :content %>



<%= f.select :rank, options_for_select(0..10), { prompt: "Select Rank" } %>



<%= f.submit %>
<% end %>
</div>
</div>
</div>
