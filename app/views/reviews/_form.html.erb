<%= form_for @review do |f| %>

<%= f.label :user %>:<br />
<%= f.collection_select :user_id, User.all, :id, :name, { prompt: "Select User" }  %>

<br /><br />

<%= f.label :club %>:<br />
<%= f.collection_select :club_id, Club.all, :id, :name, { prompt: "Select Club" }  %>

<br /><br />

<%= f.label :content %>:<br />
<%= f.text_area :content %>

<br /><br />

<%= f.label :rank %>:<br />
<%= f.select :rank, options_for_select(0..10), { prompt: "Select Rank" } %>


<br /><br />


<%= f.submit %>
<% end %>
